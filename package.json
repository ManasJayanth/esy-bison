{
  "name": "esy-bison",
  "version": "3.2.1000",
  "description": "Bison packaged for esy",
  "esy": {
    "buildsInSource": true,
    "build": [
      "./configure --prefix=#{self.install} --disable-dependency-tracking",
      "make"
    ],
    "install": [
      "make install"
    ]
  },
  "dependencies": {
    "esy-automake": "prometheansacrifice/esy-automake#0d274908e251ad6f989162f70fa497570852a684",
    "esy-autoconf": "esy-packages/esy-autoconf#94f846f3ad69fc65c65a61dd7fd3d70073dc93ea"
  }
}
